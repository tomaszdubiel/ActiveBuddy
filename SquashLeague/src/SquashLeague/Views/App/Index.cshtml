@model IEnumerable<SquashLeague.Models.Trip>
@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    <div class="col-md-6">
        <h1>Squash Leaque</h1>
        <p>This is my awesome app</p>

        <form>
            <div class="form-group">
                <label>Date</label>
                <input class="form-control" />
            </div>
            <div class="form-group">
                <label>Location</label>
                <input class="form-control" />
            </div>
            <div>
                <input type="submit" value="Add" class="btn btn-success"/>
            </div>
        </form>
    </div>
    <div class="col-md-6">
        <h2>Yo Mata</h2>
        <ul>
            @foreach (var item in Model)
            {
                <li>@item.Name : @item.DataCreated.ToString("d")</li>
            }
        </ul>
</div>
</div>
